

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="bookings" class="panel panel-default" style="margin: 2% 0;">
    <div class="panel-heading">
        <div class="row">
            <div class="col-md-6"><span style="font-weight:bold;font-size: 20px;">Bookings</span></div>
            <div class="col-md-6"></div>
        </div>
    </div>
    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover dtable" id="dtUserBookings">
                <thead>
                    <tr>
                        <th>Booking ID</th>
                        <th>Vehicle Number
                        </th>
                        <th>Customer Name
                        </th>
                        <th>Customer Mobile Number
                        </th>
                        <th>Vehicle Model
                        </th>
                        <th>Year of manufacture
                        </th>
                        <th>Service date requested
                        </th>
                        <th>Service type requested
                        </th>
                        <th>customer ID
                        </th>
                        <th>Customer email ID
                        </th>
                        <th>Details
                        </th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
</div>

<div id="feedbacks" class="panel panel-default" style="margin: 2% 0;">
    <div class="panel-heading">
        <div class="row">
            <div class="col-md-6"><span style="font-weight:bold;font-size: 20px;">Feedbacks</span></div>
            <div class="col-md-6"></div>
        </div>
    </div>
    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover dtable" id="dtUserFeedbacks">
                <thead>
                    <tr>
                      <th>Feedback ID
                        </th>
                        <th>Customer Name
                        </th>
                        <th>Customer Phone
                        </th>
                        <th>Vehicle Number
                        </th>
                        <th>Booking ID
                        </th>
                        <th>Serviced Workshop
                        </th>
                        <th>Service Type
                        </th>
                        <th>Serviced Date
                        </th>
                        <th>Feedback
                        </th>
                        <th>Details</th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
</div>
